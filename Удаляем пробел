#include <stdio.h>
#include <stdlib.h>
void func(char *str);

int main()
{

    char c [100] = "A BC  D  F";
    printf("Input: %s \n ",c);
    func(c);
    printf("result: %s \n",c );
   return 0;
}
void func (char *str){
int i=0;
int j=0;

    while (str[i]!='\0'){


        while (str[i]== ' '){
            j=i;

            while(str[j]!='\0'){
                str[j]=str[j+1];
                printf("j:%d s: %s \n ", j,str);
                j++;


                }
            }
        }
        i++;
    }


